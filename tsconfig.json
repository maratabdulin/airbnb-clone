/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "emitDecoratorMetadata": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": ["ES2022", "dom"],
    "types": ["jest"],
    "paths": {
      "@app/ui/carousel": ["src/app/ui/carousel/index.ts"],
      "@app/ui/container": ["src/app/ui/container/index.ts"],
      "@app/ui/icons": ["src/app/ui/icons/index.ts"],
      "@app/ui/grid": ["src/app/ui/grid/index.ts"],
      "@app/ui/theme/layout": ["src/app/ui/theme/layout/index.ts"],
      "@app/ui/shared": ["src/app/ui/shared/index.ts"],
      "@app/ui/spinner": ["src/app/ui/spinner/index.ts"],
      "@app/core/common": ["src/app/core/common/index.ts"],
      "@app/core/config": ["src/app/core/config/index.ts"],
      "@app/core/logger": ["src/app/core/logger/index.ts"],
      "@app/core/testing": ["src/app/core/testing/index.ts"],
      "@app/core/storage": ["src/app/core/storage/index.ts"],
      "@app/core/store/utils": ["src/app/core/store/utils/index.ts"],
      "@app/core/environments": ["src/app/core/environments/index.ts"],
      "@app/rooms/common": ["src/app/rooms/common/index.ts"],
      "@app/rooms/storage": ["src/app/rooms/storage/index.ts"],
      "@app/rooms/service": ["src/app/rooms/service/index.ts"],
      "@app/rooms/state": ["src/app/rooms/state/index.ts"],
      "@app/buildings/common": ["src/app/buildings/common/index.ts"],
      "@app/buildings/storage": ["src/app/buildings/storage/index.ts"],
      "@app/buildings/service": ["src/app/buildings/service/index.ts"],
      "@app/buildings/state": ["src/app/buildings/state/index.ts"],
      "@app/persons/common": ["src/app/persons/common/index.ts"],
      "@app/persons/storage": ["src/app/persons/storage/index.ts"],
      "@app/persons/service": ["src/app/persons/service/index.ts"],
      "@app/persons/state": ["src/app/persons/state/index.ts"],
      "@app/booking/common": ["src/app/booking/common/index.ts"],
      "@app/booking/service": ["src/app/booking/service/index.ts"],
      "@app/booking/state": ["src/app/booking/state/index.ts"],
      "@app/booking/page": ["src/app/booking/page/index.ts"],
      "@app/maps/common": ["src/app/maps/common/index.ts"],
      "@app/maps/service": ["src/app/maps/service/index.ts"],
      "@app/maps/shared": ["src/app/maps/shared/index.ts"],
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
